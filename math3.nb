Cell[BoxData[
 RowBox[{"soln", "=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"\[Tau]", "[", 
        RowBox[{"0", ",", "v0"}], "]"}], "+", "d\[Tau]"}], "\[Equal]", 
      RowBox[{"\[Tau]", "[", 
       RowBox[{"dt", ",", "v0"}], "]"}]}], ",", 
     RowBox[{"{", "dt", "}"}]}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.555289200738587*^9, 3.555289245922119*^9}, {
  3.555289520189389*^9, 3.555289535439544*^9}, {3.555289708836281*^9, 
  3.555289712325869*^9}, {3.55529065854716*^9, 3.555290671156262*^9}, {
  3.555290711990495*^9, 3.555290714278165*^9}, {3.555290928894966*^9, 
  3.555290971086215*^9}, {3.555291136020511*^9, 3.555291140196719*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{"dt", "\[Rule]", 
    FractionBox[
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{"g", " ", "v0"}], 
